<?xml version="1.0" encoding="utf-8"?>

<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">

    <CheckBoxPreference
        android:key="pref_auto_park"
        android:defaultValue="false"
        android:disableDependentsState="true"
        android:title="@string/pref_auto_park_title"
        android:summaryOn="@string/pref_auto_park_on"
        android:summaryOff="@string/pref_auto_park_off"/>

    <Preference
        android:key="pref_bluetooth"
        android:title="@string/pref_bluetooth"
        android:summary="@string/pref_bluetooth_summ"
        android:dependency="pref_auto_park"/>
</PreferenceScreen>